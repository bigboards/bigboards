[hex]
id={{ ansible_local.hex.id }}
name={{ ansible_local.hex.name }}

[network]
{% if ansible_local.hex.network is defined and ansible_local.hex.network.range is defined %}
range={{ ansible_local.hex.network.range }}
{% else %}
range=172.20.40
{% endif %}

[node]
sequence={{ ansible_local.hex.node.sequence }}
role={{ ansible_local.hex.node.role }}